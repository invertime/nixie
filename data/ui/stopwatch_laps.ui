<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="NixieStopwatchLapsRow" parent="GtkListBoxRow">
    <property name="activatable">False</property>
    <child>
      <object class="GtkBox">
        <property name="spacing">18</property>
        <property name="margin-top">18</property>
        <property name="margin-bottom">18</property>
        <property name="margin-start">18</property>
        <property name="margin-end">18</property>
        <child>
          <object class="GtkLabel" id="duration_label">
            <property name="tooltip_text">Time</property>
            <property name="halign">start</property>
            <property name="valign">center</property>
            <style>
              <class name="proton-blue" />
              <class name="numeric" />
              <class name="cb-subtitle" />
            </style>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="difference_label">
            <property name="tooltip_text">Difference</property>
            <property name="halign">start</property>
            <property name="hexpand">True</property>
            <property name="valign">center</property>
            <style>
              <class name="numeric" />
              <class name="cb-subtitle" />
            </style>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="index_label">
            <property name="halign">end</property>
            <property name="valign">center</property>
            <property name="width_chars">3</property>
            <style>
              <class name="caption" />
            </style>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
